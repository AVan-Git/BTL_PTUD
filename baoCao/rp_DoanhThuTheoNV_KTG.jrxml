		<queryString language="SQL">
			<![CDATA[SELECT TOP (10) NhanVien.maNV, NhanVien.tenNV, NhanVien.soDT, count(DISTINCT HoaDon.maHD) as 'So HD', sum( CT_HoaDon.soluong) as soluong, sum(CT_HoaDon.tongTien) as tongTien
FROM     NhanVien INNER JOIN
                  HoaDon ON NhanVien.maNV = HoaDon.maNV INNER JOIN
                  CT_HoaDon ON HoaDon.maHD = CT_HoaDon.maHD
WHERE  HoaDon.[ngaylap] >=  $P{date1}   AND  HoaDon.[ngaylap] <=   $P{date3} 
GROUP BY  NhanVien.maNV, NhanVien.tenNV, NhanVien.soDT
ORDER BY sum(CT_HoaDon.tongTien) DESC, count(DISTINCT HoaDon.maHD) DESC]]>
		</queryString>
